<%# main title container for doc partial view -%>
<!--  start rendering document header -->
<% Rails.logger.info("&^&^&^&^&^&^&^&^ @response['response']['numFound'] " + @response['response']['numFound'].inspect)%>
<% Rails.logger.info("&^&^&^&^&^&^&^&^ @document_list[0]['fulltitle_display'] " + @document_list[0]['fulltitle_display'].inspect)%>
<% Rails.logger.info("&^&^&^&^&^&^&^&^ document['pub_date_display'] " + document['pub_date_display'].inspect)%>
<h2 class="blacklight-title_display">
  <%= t('blacklight.search.documents.counter', :counter => (document_counter + 1 + @response.params[:start].to_i)) %>
  <%= link_to_document document, :label=>document_show_link_field(document), :counter => (document_counter + 1 + @response.params[:start].to_i) %>
  <span class="blacklight-pub_date_display"><%= simple_render_index_field_value :document => document, :field => 'pub_date_display' %></span>
</h2>
