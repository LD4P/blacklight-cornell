  <div class="status">
    <% if i["items"].present? %>
      <% if i["items"]["avail"].present? && noncirc == false  %>
        <span class="online-label">
          <i class="fa fa-check"></i>
          <%= I18n.t('blacklight.catalog.available') %>
        </span>
          <% if i['items']['count'] == 1 %>
            1 copy
          <% end %>
        <% elsif i["items"]["avail"].present?  and noncirc == true  %>
          <span class="online-label">
            <i class="fa fa-check fa-on-site" title="On-site use"></i>
            <%= I18n.t('blacklight.catalog.available') %>
          </span>
          <%= I18n.t('blacklight.catalog.on_site') %>
        <% elsif i['boundWith'].present? %>
            <% i['boundWith'].each do |b,v| %>
              t('blacklight.catalog.bound_with_status_label') <%=   link_to v["masterTitle"]+" "+v["masterEnum"], "/catalog/" + v["masterBibId"] %>
        <% end %>

      <% elsif i["items"]["unavail"].present? %>
      <i class="fa fa-clock-o"></i>
      <% if i['items']['count'] == 1 %>
        c. 1
      <% end %>
        <% i["items"]["unavail"].each do |item| %>
          <% if item["status"]["due"].present? %>
            Checked out, due <%= Time.at(item["status"]["due"]).strftime("%F") %>
            <%  item["status"] = "checked_out" %>
        <%end %>
        <% if item["recalls"].present? %>
        (Requests: <%= item["recalls"] %>)
        <% end %>
      <%end %>
    <% end %>
    <% if i["items"]["count"] >1 and !multi_vol %>
       <%= "#{items['count']} copies" %>
       <% end %>
    <% if i["items"]["count"] >1 and multi_vol %>
       <%= "#{i["items"]["count"]} volumes/copies" %>
        <% end %>
    <% if i["items"]["avail"].present? && i["items"]["avail"] != i["items"]["count"] %>
      <% some_av = 'yes' %>
    <% end %>
    <% end %>

    <% if i["order"].present?  %>
    <i class="fa fa-clock-o"></i> <%= I18n.t('blacklight.catalog.on_order') %>
    <% end %>
</div>
