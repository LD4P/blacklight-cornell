<h1>Browse subject eras</h1>

<table>
  <thead>
    <tr>
      <th>Era</th>
      <th>URI</th>
    </tr>
  </thead>
  <tbody id="subject-eras">
    <% for era in @subject_eras %>
      <tr>
        <td><%= link_to era['label'], '/?f[fast_era_facet][]=' + era['label'].gsub('-', '+-+') %></td>
        <td class="uri" data="<%= era['uri'] %>"><%= era['uri'] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
